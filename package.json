{
  "name": "youtube_clone",
  "version": "1.0.0",
  "description": "A MERN clone of the popular cat video consumption platform",
  "main": "index.js",
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "clsx": "^1.1.1",
    "concurrently": "^6.2.0",
    "npm": "^7.5.4",
    "react-iframe": "^1.8.0"
  },
  "scripts": {
    "start": "node server.js --prefix backend",
    "server": "cd backend && nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmorrisey/YouTube_Clone.git"
  },
  "keywords": [
    "YouTube",
    "MERN",
    "React",
    "Clone"
  ],
